{
  "expo": {
    "name": "AnxietyCrush",
    "slug": "anxiety-crush",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anxietycrush.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anxietycrush.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "AnxietyCrush",
      "shortName": "AnxietyCrush",
      "lang": "en",
      "scope": "/",
      "themeColor": "#ffffff",
      "backgroundColor": "#ffffff",
      "startUrl": "/?source=pwa",
      "display": "standalone",
      "orientation": "portrait",
      "relatedApplications": [],
      "preferRelatedApplications": false,
      "description": "Anxiety management app to help you crush anxiety",
      "bundler": "metro",
      "output": "single",
      "optimization": {
        "minify": true,
        "splitChunks": true,
        "treeshake": true
      }
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "scheme": "anxiety-crush"
  }
}
